<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.bluemixdata" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="IBMBluemixHybrid">
        <param name="android-package" value="com.ibm.mobile.services.cordova.core.CDVIBMBluemix" />
    </feature>
    <feature name="IBMDataHybrid">
        <param name="android-package" value="com.ibm.mobile.services.cordova.data.CDVIBMData" />
    </feature>
    <feature name="IBMQueryHybrid">
        <param name="android-package" value="com.ibm.mobile.services.cordova.data.CDVIBMQuery" />
    </feature>
    <name>BluemixData</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
